put 'Student','2015001','course','Math'
put 'Student','2015002','course','Chinese'
put 'Student','2015003','course','Math'


put 'Student','2015002','S_Name','Mary'
put 'Student','2015002','S_Sex','female'
put 'Student','2015002','S_Age','22'

scan  'Student', {COLUMNS => 'course'}

put 'Student','2015001','course','Physics'
put 'Student','2015003','course','Physics'

count 'Student'

alter 'Student', {NAME => 'S_Age', METHOD => 'delete'}
